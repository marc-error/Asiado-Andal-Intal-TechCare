  <style>

    
    /* Card split into two columns: left = media, right = content.
       Responsive: stacks to single column on small screens.
       Divider between halves on wide viewports. */
    .guide-grid {
      display: grid;
      gap: 1rem;
      margin-bottom: 2rem;
    }

    .blog-post-card {
      display: grid;
      grid-template-columns: 360px 1fr; /* left column fixed (media), right column flexible */
      gap: 0;
      align-items: stretch;
      border-radius: 12px;
      border: 1px solid var(--card-border, #e6e6e6);
      background: var(--card, #fff);
      overflow: hidden;
      transition: transform .18s ease, box-shadow .18s ease, border-color .18s ease;
      cursor: pointer;
      text-decoration: none;
      color: inherit;
    }

    /* Left side (media) */
    .post-image {
      width: 100%;
      height: 100%;
      min-height: 180px;
      display: block;
      background: #f3f3f3;
      overflow: hidden;
      position: relative;
    }
    .post-image img {
      width: 100%;
      height: 100%;
      display: block;
      object-fit: cover;
      transition: transform .35s ease;
    }
    .blog-post-card:hover .post-image img {
      transform: scale(1.03);
    }

    /* Right side (content) */
    .post-content {
      padding: 1.25rem 1.5rem;
      display: flex;
      flex-direction: column;
      justify-content: space-between;
      border-left: 1px solid var(--card-border, #e6e6e6); /* visual divider */
      background: linear-gradient(180deg, rgba(255,255,255,0.01), transparent);
    }

    .post-meta { color: var(--muted-foreground); font-size: .9rem; display:flex; gap:.5rem; align-items:center; margin-bottom:.5rem; }
    .post-title { margin: 0 0 .5rem 0; font-size: 1.125rem; font-weight: 700; color: var(--foreground); }
    .post-excerpt { margin: 0 0 1rem 0; color: var(--muted-foreground); line-height:1.6; flex:1; }

    .post-footer { display:flex; justify-content:space-between; align-items:center; gap:.5rem; margin-top: 1rem; }
    .open-modal { border:1px solid var(--card-border); background:transparent; padding:.35rem .6rem; border-radius:.375rem; font-size:.875rem; cursor:pointer; color:var(--foreground); }

    /* Hover lift */
    .blog-post-card:hover {
      transform: translateY(-6px);
      box-shadow: 0 10px 30px rgba(16,24,40,0.08);
      border-color: rgba(37,99,235,0.12);
    }

    /* Responsive: on small screens stack media above content, remove divider */
    @media (max-width: 880px) {
      .blog-post-card {
        grid-template-columns: 1fr;
      }
      .post-image {
        min-height: 200px;
      }
      .post-content {
        border-left: none;
        padding: 1rem;
      }
    }

    /* ===========================
       Modal centering & behavior
       Ensure the modal overlay centers the dialog horizontally and vertically.
       Also add subtle animation and prevent background scrolling while open.
       =========================== */
    .modal-overlay {
      position: fixed;
      inset: 0;
      display: none;
      align-items: center;
      justify-content: center;
      z-index: 9999;
      padding: 1.25rem;
      background: rgba(2,6,23,0.55);
      backdrop-filter: blur(6px);
    }
    /* When aria-hidden is false, show overlay as a flex container and center children */
    .modal-overlay[aria-hidden="false"] {
      display: flex;
    }
    .modal {
      width: min(900px, 100%);
      max-height: 90vh;
      overflow: auto;
      border-radius: 12px;
      background: var(--card, #fff);
      box-shadow: 0 18px 50px rgba(2,6,23,0.5);
      border: 1px solid var(--card-border, rgba(0,0,0,0.08));
      padding: 1rem;
      position: relative;
      transform-origin: center;
      animation: modalPop .16s ease;
    }
    @keyframes modalPop {
      from { transform: translateY(-6px) scale(.995); opacity: 0; }
      to   { transform: translateY(0) scale(1); opacity: 1; }
    }
    /* Ensure focus outline is visible */
    .modal:focus { outline: none; box-shadow: 0 0 0 4px rgba(37,99,235,0.08); }

  </style>
</head>
