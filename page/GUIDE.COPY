  <script>
    // Minimal modal script (works with modal-trigger cards & "open-modal" buttons)
    (function(){
      const overlay = document.getElementById('modal-overlay');
      const modal = overlay && overlay.querySelector('.modal');
      const titleEl = document.getElementById('modal-title');
      const catEl = document.getElementById('modal-category');
      const descEl = document.getElementById('modal-desc');
      const mediaEl = document.getElementById('modal-media');
      const linkEl = document.getElementById('modal-link');

      // Save previously focused element to restore focus on close
      let previousActiveElement = null;

      function openModal(data) {
        previousActiveElement = document.activeElement;
        titleEl.textContent = data.title || '';
        catEl.textContent = data.category || '';
        descEl.textContent = data.desc || '';
        linkEl.href = data.video || '#';

        // inject iframe (YouTube) if video present
        mediaEl.innerHTML = '';
        if (data.video && data.video.includes('youtube')) {
          try {
            const url = new URL(data.video);
            const vid = url.searchParams.get('v') || (data.video.split('/').pop());
            const iframe = document.createElement('iframe');
            iframe.width = '100%';
            iframe.height = '360';
            iframe.src = 'https://www.youtube.com/embed/' + vid + '?autoplay=1';
            iframe.title = data.title || 'Video';
            iframe.frameBorder = '0';
            iframe.allow = 'accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture';
            iframe.allowFullscreen = true;
            mediaEl.appendChild(iframe);
          } catch (e) {
            const a = document.createElement('a');
            a.href = data.video;
            a.textContent = 'Open media';
            a.target = '_blank';
            mediaEl.appendChild(a);
          }
        } else if (data.video) {
          const a = document.createElement('a');
          a.href = data.video;
          a.textContent = 'Open media';
          a.target = '_blank';
          mediaEl.appendChild(a);
        }

        // show overlay and center modal via CSS
        overlay.style.display = 'flex';
        overlay.setAttribute('aria-hidden','false');

        // prevent background scroll
        document.documentElement.style.overflow = 'hidden';
        document.body.style.overflow = 'hidden';

        // focus the modal for keyboard users
        modal.setAttribute('tabindex','-1');
        modal.focus();
      }

      function closeModal() {
        overlay.style.display = 'none';
        overlay.setAttribute('aria-hidden','true');
        mediaEl.innerHTML = '';

        // restore scrolling
        document.documentElement.style.overflow = '';
        document.body.style.overflow = '';

        // restore focus to previously focused element
        if (previousActiveElement && typeof previousActiveElement.focus === 'function') {
          previousActiveElement.focus();
        }
      }

      // attach handlers to card clicks and quick view buttons
      document.querySelectorAll('.modal-trigger').forEach(card => {
        card.addEventListener('click', (ev) => {
          // ignore clicks on links inside card
          if (ev.target.closest('a')) return;
          const data = {
            category: card.dataset.category,
            title: card.dataset.title,
            desc: card.dataset.desc,
            video: card.dataset.video
          };
          openModal(data);
        });
      });

      document.querySelectorAll('.open-modal').forEach(btn => {
        btn.addEventListener('click', (ev) => {
          ev.stopPropagation();
          const card = ev.target.closest('.blog-post-card');
          if (!card) return;
          const data = {
            category: card.dataset.category,
            title: card.dataset.title,
            desc: card.dataset.desc,
            video: card.dataset.video
          };
          openModal(data);
        });
      });

      // close buttons and overlay click
      overlay.addEventListener('click', (ev) => {
        if (ev.target === overlay) closeModal();
      });
      overlay.querySelectorAll('.modal-close').forEach(b => b.addEventListener('click', closeModal));

      // keyboard: ESC to close, trap focus inside modal while open
      document.addEventListener('keydown', (ev) => {
        if (ev.key === 'Escape' && overlay.style.display === 'flex') {
          closeModal();
        }
        // simple focus trap: if modal is open and Tab is pressed, keep focus inside modal
        if (ev.key === 'Tab' && overlay.style.display === 'flex') {
          const focusable = modal.querySelectorAll('a[href], button:not([disabled]), textarea, input, select, iframe, [tabindex]:not([tabindex="-1"])');
          if (!focusable.length) return;
          const first = focusable[0];
          const last = focusable[focusable.length - 1];
          if (ev.shiftKey) { // shift + tab
            if (document.activeElement === first) {
              ev.preventDefault();
              last.focus();
            }
          } else { // tab
            if (document.activeElement === last) {
              ev.preventDefault();
              first.focus();
            }
          }
        }
      });
    })();
  </script>
</body>
</html>